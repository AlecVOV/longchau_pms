{
  "name": "pharmacy-ecommerce",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "dotenv -e .env -- nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:seed": "dotenv -e .env -- tsx scripts/seed-simple.ts",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:reset": "dotenv -e .env -- prisma db push --force-reset && npm run db:seed"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@iconify-json/ph": "^1.2.2",
    "@nuxt/image": "^1.0.0",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "^6.13.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "nuxt": "^3.13.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.366",
    "@iconify/vue": "^4.1.1",
    "@nuxt/devtools": "latest",
    "@nuxtjs/color-mode": "^3.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@unocss/nuxt": "^66.4.1",
    "@vueuse/core": "^10.3.0",
    "@vueuse/nuxt": "^10.3.0",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.13.0",
    "tsx": "^4.20.3"
  }
}
